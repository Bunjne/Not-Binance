<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.genxaswhiz.notbiance">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".NotBinance"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NotBinance">
        <activity android:name=".presentation.main.NewsWebsiteActivity"></activity>
        <activity android:name=".presentation.main.NewsDescriptionActivity" />
        <activity android:name=".presentation.main.CoinDescriptionActivity" />
        <activity android:name=".presentation.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>